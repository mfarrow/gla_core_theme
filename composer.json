{
    "name": "gla/gla_core_theme",
    "type": "gla-estate-theme",
    "description": "Core GLA estate Drupal theme",
    "license": "OGL-UK-3.0",
    "require": {
        "composer/installers": "^1.10",
        "cweagans/composer-patches": "^1.7",
        "npm-asset/alpinejs": "^3.2",
        "npm-asset/basicscroll": "^3.0",
        "npm-asset/lazysizes": "^5.3",
        "npm-asset/lite-youtube-embed": "^0.2.0",
        "npm-asset/lozad": "^1.16",
        "npm-asset/svgxuse": "^1.2",
        "npm-asset/what-input": "^5.2",
        "npm-asset/wicg-inert": "^3.1",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "vijaycs85/drupal-quality-checker": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "libraries/{$name}": [
                "type:npm-asset"
            ]
        },
        "installer-types": [
            "npm-asset"
        ],
        "patches": {}
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lukewhitmore/drupal-quality-checker"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
